	<parameter name="RECORD_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT cl.created fecha,u.name usuario,cl.characterdata mensaje FROM cm_chatentry cl
join cm_chat c on c.cm_chat_id=cl.cm_chat_id
join ad_user u on u.ad_user_id=cl.createdby
where cl.isactive='Y' and c.ad_table_id=259 and c.record_id=$P{RECORD_ID} order by cl.created]]>
	</queryString>
	<field name="fecha" class="java.sql.Timestamp"/>
	<field name="usuario" class="java.lang.String"/>
	<field name="mensaje" class="java.lang.String"/>
