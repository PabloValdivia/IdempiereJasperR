	<queryString>
		<![CDATA[select
	org.name as org,
	coalesce(a.name, 'Sin Localidad') as localidad,
	to_char(i.dateinvoiced, 'yyyy/MM') as fechaFactura,
	(case 
		when dt.DocBaseType = 'ARC' then -il.linenetamt else il.linenetamt end
	) as subTotalLinea,
	(case 
		when dt.DocBaseType = 'ARC' then -il.TaxAmt else il.TaxAmt end
	) as impuestoLinea,
	(case 
		when dt.DocBaseType = 'ARC' then -il.LineTotalAmt else il.LineTotalAmt end
	) as totalLinea
from C_Invoice i
inner join C_InvoiceLine il on il.C_Invoice_ID = i.C_Invoice_ID and il.isactive = 'Y'
JOIN C_DocType dt ON (dt.C_DocType_ID = i.C_DocTypeTarget_ID) and dt.DocBaseType in ('ARI', 'ARC')
inner join AD_Org org on org.ad_org_id = i.ad_org_id and org.isactive='Y'
left join C_Activity as a on a.C_Activity_ID = i.C_Activity_ID and a.isactive = 'Y'
where i.issotrx='Y' and i.DocStatus='CO' and i.isactive = 'Y'
and i.dateinvoiced between date_trunc('month', date( $P{fecha_inicio} ))
	and (date_trunc('month', date(  $P{fecha_fin}  )) + interval '1 month' - interval '1 second')
and i.ad_org_id =  $P{AD_Org_ID} 
and coalesce( $P{M_Product_ID} , -1) in (il.M_Product_ID, -1)
and i.docstatus in ('CO', 'CL')
and i.issotrx = 'Y'
order by org, localidad, fechaFactura, totalLinea]]>
	</queryString>
