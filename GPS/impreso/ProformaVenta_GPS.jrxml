	<queryString>
		<![CDATA[select
	vw_org.logo,
	vw_org.taxid idRUC,
	o.AD_Org_ID::integer,
	vw_org.name as Organizacion,
	vw_org.localizacion as Direccion1,
	vw_org.phone,
	vw_org.email,
	vw_org.url,
	o.DocumentNo,
	o.DateOrdered,
	t.name as terminoPago,
	c.name as nombreTercero,
	c_loc_ad.Address1 as direccion_tercero,
	tit.description as tipo,
	c.taxid as ID,
	us.name as contacto_cliente,
	us.email as email_contacto_cliente,
	us.phone as telefono_contacto_cliente,
	us.phone2 as celular_contacto_cliente,
	'' as nombre_contacto_tecnico_cliente,
	'' as descripcion_contacto_tecnico_cliente,
	'' as email_contacto_tecnico_cliente,
	'' as telefono_contacto_tecnico_cliente,
	coalesce(r.description, r.name) as representante,
	j.name posicionrep,
	r.email emailrep,
	r.phone telefonorep,
	r.phone2 celularrep,
	o.Description as concepto,
	p.m_product_id,
	coalesce(ch.name,p.value) as nombreserviciopro,
	coalesce(ol.description,bp.description,p.description,p.name,ch.description,ch.name) descriServiciopro,
	ol.nota as nota,
	coalesce(ut.uomsymbol,u.uomsymbol) unidad,
	trunc(ol.QtyEntered) cantidad,
	ol.linenetamt,
	ol.pricelist precio,
	ol.PriceEntered precioConDescuento,
	ol.Discount porcentajeDescuento,
	round(coalesce(ol.Discount, 0)*(ol.linenetamt)/100,2) as descuento,
	tax.Rate,
	'' as PorcentajeIVA,
	ol.Discount,
	round(coalesce(tax.Rate, 0)*(ol.linenetamt)/100,2) as iva,
	o.grandtotal,
	tn.description notas,
	upper(dt.printname) tipodocumento,
	cot.documentno cotizacion,
	dt.DocSubTypeSO,
	'' as validez,
	'' as fecharequerida,
	p.GuaranteeDays diasgarantia,
	'' as tiempoentrega,
	coalesce(pc.name,'OTROS') categoria,
	act.name as centroCosto,
	o.description as descripcion,
	'' as referenciaPedido
from C_Order o
left join c_paymentTerm t on t.C_PaymentTerm_ID=o.C_PaymentTerm_ID
left join  c_bpartner c on c.c_bpartner_id=o.c_bpartner_id
left join C_BPartner_Location as c_loc on c_loc.C_BPartner_Location_ID = o.C_BPartner_Location_ID
left join C_Location as c_loc_ad on c_loc_ad.C_Location_ID = c_loc.C_Location_ID
left join c_orderline ol on ol.C_order_ID=o.C_order_ID
left join LCO_TaxIdType tit on tit.LCO_TaxIdType_id=c.LCO_TaxIdType_id
left join m_product p on p.m_product_id=ol.m_product_id
left join M_Product_Category pc on pc.M_Product_Category_ID=p.M_Product_Category_ID
left join C_BPartner_Product bp on bp.c_bpartner_id=c.c_bpartner_id and bp.m_product_id=p.m_product_id
left join C_Charge ch on ch.c_charge_id=ol.c_charge_id
left join c_tax tax on ol.C_Tax_ID=tax.C_Tax_ID
inner join vw_org on vw_org.AD_Org_ID = o.AD_Org_ID
left join c_uom u on u.c_uom_id=p.c_uom_id
left join c_uom_trl ut on ut.c_uom_id=u.c_uom_id and ut.ad_language='es_EC' and ut.isactive='Y'
left join ad_user r on r.ad_user_id=o.salesrep_id
left join c_job j on j.c_job_id=r.c_job_id
left join ad_user us on us.ad_user_id=o.ad_user_id
left join tb_notas tn on tn.tb_notas_ID=o.tb_notas_ID
left join C_DocType dt on dt.C_DocType_id=o.C_DocTypetarget_id
left join c_order cot on o.quotationorder_id=cot.C_order_ID
left join C_Activity act on act.C_Activity_ID = o.C_Activity_ID and act.isactive = 'Y'
where o.issotrx='Y' and o.c_order_id=$P{C_Order_ID} order by pc.name,ol.line]]>
